<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Our Team - KezaTech</title>
    <link rel="icon" type="image/favicon.png" href="/images/favicon.png">
    <link rel="stylesheet" href="/styles/style.css" />
  </head>
  <body>
    <!-- Include Navigation Bar -->
    <%- include('nav.html') %>

    <!-- Team Hero Section -->
    <section class="team-hero">
      <div class="team-hero-content">
        <h1>Our Team</h1>
        <p>Meet the passionate individuals driving innovation in healthcare technology</p>
      </div>
    </section>

    <!-- Team Members Section -->
    <section class="team-members">
      <div class="team-grid">
        <!-- Team Member 1 -->
        <div class="team-member">
          <div class="member-image">
            <img src="/images/TEAM/Keeza.jpeg" alt="Yvonne Keeza">
          </div>
          <div class="member-info">
            <h3>Yvonne Keeza</h3>
            <p class="position">Chief Executive Officer</p>
            <p class="bio">Medical Imaging Specialist</p>
            <div class="member-social">
              <a href="#" class="social-link"><i class="fab fa-linkedin"></i></a>
              <a href="#" class="social-link"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>

        <!-- Team Member 2 -->
        <div class="team-member">
          <div class="member-image">
            <img src="/images/TEAM/Prince%20Rukundo.jpeg" alt="Prince RUKUNDO">
          </div>
          <div class="member-info">
            <h3>Prince RUKUNDO</h3>
            <p class="position">Data scientist & AI Engineer</p>
            <div class="member-social">
              <a href="#" class="social-link"><i class="fab fa-linkedin"></i></a>
              <a href="#" class="social-link"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>

        <!-- Team Member 3 -->
        <div class="team-member">
          <div class="member-image">
            <img src="/images/TEAM/Patrick-RD.png" alt="Dr Patrick NYISHAKA">
          </div>
          <div class="member-info">
            <h3>Dr Patrick NYISHAKA</h3>
            <p class="position">Data Scientist & AI Engineer</p>
            <div class="member-social">
              <a href="#" class="social-link"><i class="fab fa-linkedin"></i></a>
              <a href="#" class="social-link"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>

        <!-- Team Member 4 -->
        <div class="team-member">
          <div class="member-image">
            <img src="/images/TEAM/AKIRADAWE.jpeg" alt="Akradawe Christian">
          </div>
          <div class="member-info">
            <h3>Akradawe Christian</h3>
            <p class="position">Information Technology</p>
            <div class="member-social">
              <a href="#" class="social-link"><i class="fab fa-linkedin"></i></a>
              <a href="#" class="social-link"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>

        <!-- Team Member 5 -->
        <div class="team-member">
          <div class="member-image">
            <img src="/images/TEAM/Ezechiel%20IRANKUNDA.jpg" alt="Ezechiel IRANKUNDA">
          </div>
          <div class="member-info">
            <h3>Ezechiel IRANKUNDA</h3>
            <p class="position">Cyber Security & Fullstack Engineer</p>
            <div class="member-social">
              <a href="#" class="social-link"><i class="fab fa-linkedin"></i></a>
              <a href="#" class="social-link"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Values Section -->
    <section class="team-values">
      <div class="values-content">
        <h2>Our Values</h2>
        <div class="values-grid">
          <div class="value-item">
            <h3>Innovation</h3>
            <p>Pushing boundaries in healthcare technology</p>
          </div>
          <div class="value-item">
            <h3>Excellence</h3>
            <p>Delivering the highest quality solutions</p>
          </div>
          <div class="value-item">
            <h3>Integrity</h3>
            <p>Maintaining the highest ethical standards</p>
          </div>
        </div>
      </div>
    </section>

    <!-- Include Footer -->
    <%- include('footer.ejs') %>
  </body>
</html> 